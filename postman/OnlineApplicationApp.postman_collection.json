					"let token = pm.globals.get(\"jwt_token\");\r",
					"let excludedPaths = [\r",
					"    \"/user/create\",\r",
					"    \"/user/login\",\r",
					"    \"/webhook/delivery-status\",\r",
					"    \"/order/webhook/update-status\"\r",
					"];\r",
					"// Get current request path\r",
					"let requestUrl = pm.request.url.getPath();\r",
					"// console.log(\"request url is \"+requestUrl);\r",
					"\r",
					"\r",
					"// Check if current path is in excluded list\r",
					"if (!excludedPaths.includes(requestUrl)) {\r",
					"    if (token) {\r",
					"        pm.request.headers.add({\r",
					"            key: \"Authorization\",\r",
					"            value: `Bearer ${token}`\r",
					"        });\r",
					"    }\r",
					"}\r",
					""
				]
			}
